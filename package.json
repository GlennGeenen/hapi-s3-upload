    "utils.js"
  ],
  "dependencies": {
    "aws-sdk": "2.x.x",
    "boom": "*"
  },
  "devDependencies": {
    "assert": "1.x.x",
    "eslint": "3.x.x",
    "eslint-config-hapi": "10.x.x",
    "eslint-plugin-hapi": "4.x.x",
    "form-data": "2.x.x",
    "hapi": "15.x.x",
